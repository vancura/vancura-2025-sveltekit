{
  "name": "@vancura/shared-ui",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "exports": {
    ".": {
      "svelte": "./src/lib/index.ts",
      "default": "./src/lib/index.ts"
    },
    "./components/*": {
      "svelte": "./src/lib/components/*",
      "default": "./src/lib/components/*"
    },
    "./layouts/*": {
      "svelte": "./src/lib/layouts/*",
      "default": "./src/lib/layouts/*"
    },
    "./styles/*": {
      "default": "./src/lib/styles/*"
    }
  },
  "svelte": "./src/lib/index.ts",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint . --fix",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^9.0.12",
    "@storybook/addon-docs": "^9.0.12",
    "@storybook/addon-links": "^9.0.12",
    "@storybook/addon-svelte-csf": "^5.0.3",
    "@storybook/addon-themes": "^9.0.12",
    "@storybook/svelte": "^9.0.5",
    "@storybook/svelte-vite": "^9.0.12",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.10",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "storybook": "^9.0.12",
    "svelte": "^5.34.7",
    "svelte-check": "^4.2.2",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  }
}